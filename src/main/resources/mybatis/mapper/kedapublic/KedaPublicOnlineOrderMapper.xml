<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hrtxn.ringtone.project.threenets.kedas.kedapublic.mapper.KedaPublicOnlineOrderMapper">

    <resultMap id="BaseResultMap"
               type="com.hrtxn.ringtone.project.threenets.kedas.kedapublic.domain.KedaPublicOnlineOrder">
        <id column="id" property="id" jdbcType="INTEGER"/>
        <result column="name" property="name" jdbcType="VARCHAR"/>
        <result column="phone" property="phone" jdbcType="VARCHAR"/>
        <result column="company_name" property="companyName" jdbcType="VARCHAR"/>
        <result column="create_time" property="createTime" jdbcType="TIMESTAMP"/>
        <result column="create_time" property="createTime" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
      id, name, phone, company_name,create_time
  </sql>

    <!--添加商务彩铃在线办理订单-->
    <insert id="insertOnlieOrder"
            parameterType="com.hrtxn.ringtone.project.threenets.kedas.kedapublic.domain.KedaPublicOnlineOrder">
        insert into tb_kedapublic_online_order (name, phone, company_name)
        values ( #{name,jdbcType=VARCHAR}, #{phone,jdbcType=VARCHAR}, #{companyName,jdbcType=VARCHAR})
    </insert>


    <select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterType="java.lang.Integer">
        select
        <include refid="Base_Column_List"/>
        from tb_kedapublic_online_order
        where id = #{id,jdbcType=INTEGER}
    </select>

    <delete id="deleteByPrimaryKey" parameterType="java.lang.Integer">
        delete from tb_kedapublic_online_order
        where id = #{id,jdbcType=INTEGER}
    </delete>
</mapper>