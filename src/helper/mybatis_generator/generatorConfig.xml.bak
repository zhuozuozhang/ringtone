		<table schema="grammar" tableName="tbl_version_seq" domainObjectName="VersionSeqEntity">
			 <property name="useActualColumnNames" value="false"/>
		     <generatedKey column="key" sqlStatement="MySql" identity="false"  />
		</table>
		<table schema="grammar" tableName="tbl_video" domainObjectName="VideoEntity">
			 <property name="useActualColumnNames" value="false"/>
		     <generatedKey column="video_id" sqlStatement="MySql" identity="true"  />
		</table>
		<table schema="grammar" tableName="tbl_grammar" domainObjectName="GrammarEntity">
