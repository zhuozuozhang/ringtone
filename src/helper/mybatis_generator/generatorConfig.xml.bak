		<table schema="grammar" tableName="tbl_function" domainObjectName="FunctionEntity">
			 <property name="useActualColumnNames" value="false"/>
		     <generatedKey column="function_id" sqlStatement="MySql" identity="true"  />
		</table>
		<table schema="grammar" tableName="tbl_organization" domainObjectName="OrganizationEntity">
			 <property name="useActualColumnNames" value="false"/>
		     <generatedKey column="id" sqlStatement="MySql" identity="true"  />
		</table>
		<table schema="grammar" tableName="tbl_role" domainObjectName="RoleEntity">
			 <property name="useActualColumnNames" value="false"/>
		     <generatedKey column="role_id" sqlStatement="MySql" identity="true"  />
		</table>
		<table schema="grammar" tableName="tbl_sys_info" domainObjectName="SysInfoEntity">
			 <property name="useActualColumnNames" value="false"/>
