			 <property name="useActualColumnNames" value="false"/>
		     <generatedKey column="type_id" sqlStatement="MySql" identity="true"  />
		</table>
		<table schema="grammar" tableName="tbl_keyword_other_name" domainObjectName="KeywordOtherNameEntity">
			 <property name="useActualColumnNames" value="false"/>
		     <generatedKey column="keyword_other_name_id" sqlStatement="MySql" identity="true"  />
		</table>
		<table schema="grammar" tableName="tbl_keyword" domainObjectName="KeywordEntity">
			 <property name="useActualColumnNames" value="false"/>
		     <generatedKey column="keyword_id" sqlStatement="MySql" identity="true"  />
		</table>-->
		<table schema="grammar" tableName="tbl_grammar" domainObjectName="GrammarEntity">
			 <property name="useActualColumnNames" value="false"/>
		     <generatedKey column="grammar_id" sqlStatement="MySql" identity="true"  />
		</table>
	</context>
</generatorConfiguration>