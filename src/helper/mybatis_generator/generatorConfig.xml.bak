		</table>
		<table schema="grammar" tableName="tbl_grammar_explain" domainObjectName="GrammarExplainEntity">
			 <property name="useActualColumnNames" value="false"/>
		     <generatedKey column="grammar_explain_id" sqlStatement="MySql" identity="true"  />
		</table>
		<table schema="grammar" tableName="tbl_grammar_keyword" domainObjectName="GrammarKeywordEntity">
			 <property name="useActualColumnNames" value="false"/>
		     <generatedKey column="grammar_keyword_id" sqlStatement="MySql" identity="true"  />
		</table>
		<table schema="grammar" tableName="tbl_teacher" domainObjectName="TeacherEntity">
			 <property name="useActualColumnNames" value="false"/>
